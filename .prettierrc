{
  "printWidth": 80,
  "tabWidth": 4,
  "useTabs": false,
  "semi": false,
  "singleQuote": false,
  "trailingComma": "es5",
  "bracketSpacing": true,
  "bracketSameLine": false,
  "arrowParens": "always",
  "requirePragma": false,
  "insertPragma": false,
  "proseWrap": "preserve",
  "overrides": [
    {
      "files": "*.js",
      "options": {
        "parser": "babel"
      }
    },
    {"files": "*.css",
      "options": {
        "parser": "css"
      }
    },
    {"files": "*.scss",
      "options": {
        "parser": "scss"
      }
    },
    {"files": "*.json",
    "options": {
      "printWidth": 120,
    }
    }
  ]
}
